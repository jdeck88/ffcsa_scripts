<berkeleymapper>
  <metadata>
    <name>FFCSA Configuration File</name>
    <relatedinformation>https://deckfamilyfarm.com/</relatedinformation>
    <abstract>Configuration file for FFCSA</abstract>
    <linkbackheader location="https://deckfamilyfarm.com/"/>
    <legendText>
      <![CDATA[ Full Farm CSA dropsite locations.  You may pickup your totes at one of the indicated dropsites for free.  Home delivery, with a charge is provided in the grey areas on the map. Use the address lookup below to see if you qualify for home delivery.]]>
    </legendText>
  </metadata>
	<concepts>
		<concept order="1" viewlist="1" datatype="char120:1" alias="Name"/>
		<concept order="2" viewlist="1" datatype="char120:2" alias="Day"/>
		<concept order="3" viewlist="1" datatype="char120:3" alias="Address"/>
		<concept order="4" viewlist="1" datatype="char120:4" alias="Time"/>
		<concept order="12" viewlist="0" datatype="darwin:decimallatitude" alias="Decimal Latitude"/>
		<concept order="13" viewlist="0" datatype="darwin:decimallongitude" alias="Decimal Longitude"/>
	</concepts>
	<gisdata>
	<layer title="Dropsite Delivery Area (you must live in this area to qualify for home delivery)" legend="1" active="1" >
	<![CDATA[https://raw.githubusercontent.com/jdeck88/ffcsa_scripts/dropsite_maps/dropsites.kmz]]>
	</layer>
	</gisdata>
	<logos>
		<logo img="https://github.com/jdeck88/ffcsa_scripts/blob/7d438482384d0f7b0aafceb417aea57bc7180aeb/dropsite_maps/csalogo.jpeg?raw=true" url="https://www.deckfamilyfarm.com/"/>
	</logos>
